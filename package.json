{
  "name": "kitsune-web",
  "version": "1.0.0",
  "author": "Edward Nicholes Jr.",
  "license": "GPL-3.0-only",
  "scripts": {
    "build": "webpack",
    "clean": "rm -rf ./.nyc_output ./.tmp ./coverage ./dist",
    "serve": "NODE_ENV=development webpack-dev-server --hot",
    "test": "NODE_ENV=test webpack && nyc --reporter=html --include 'src/**/*.js' mocha --require source-map-support/register dist/spec.js",
    "test:debug": "NODE_ENV=test webpack && nyc --reporter=html --include 'src/**/*.js' mocha --inspect-brk --require source-map-support/register dist/spec.js",
    "test:watch": "NODE_ENV=test mocha-webpack --require source-map-support/register --watch --webpack-config webpack.config.js src/index.spec.js"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.12.0",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-config-xo-space": "^0.20.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.12.3",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "jssha": "^2.3.1",
    "lodash": "^4.17.11",
    "mocha": "^5.2.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "node-sass": "^4.11.0",
    "nyc": "^13.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-hot-loader": "^4.6.3",
    "react-redux": "^6.0.0",
    "react-test-renderer": "^16.7.0",
    "redux": "^4.0.1",
    "sass-loader": "^7.1.0",
    "screenfull": "^4.0.0",
    "should": "^13.2.3",
    "sinon": "^7.2.2",
    "source-map-support": "^0.5.10",
    "style-loader": "^0.23.1",
    "toastr": "^2.1.4",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
